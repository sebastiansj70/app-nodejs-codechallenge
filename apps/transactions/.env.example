# Database Configuration
DB_TYPE=postgres
DB_HOST=your_database_host_here 
DB_PORT=5432
DB_USERNAME=your_db_username_here  
DB_PASSWORD=your_db_password_here 
DB_DATABASE=your_db_name_here 
DB_SYNCHRONIZE=true  
