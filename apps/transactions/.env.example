# Kafka Topics
KAFKA_TOPIC_TRANSACTIONS=your_transactions_topic_here 
KAFKA_TOPIC_UPDATE=your_update_topic_here 

# Configuración del Módulo de Transacciones
KAFKA_CLIENT_ID_TRANSACTIONS=your_client_id_here 
KAFKA_GROUP_ID_TRANSACTIONS=your_group_id_here  
